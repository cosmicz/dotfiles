#!/usr/bin/env bash
## -*- mode: sh-mode; coding: utf-8 -*-

#
# alias
#


#
# Bashmarks - https://github.com/huyng/bashmarks
#

[ -f ~/.local/bin/bashmarks.sh ] && source ~/.local/bin/bashmarks.sh


#
# Java
#

export PATH=$JAVA_HOME/bin:$PATH


#
# Android SDK
#

export PATH=$ANDROID_HOME/build-tools/25.0.2:$PATH


#
# Android NDK
#

export ANDROID_NDK_HOME=$HOME/Library/android-ndk-r10b
export PATH=$ANDROID_NDK_HOME:$PATH


# Android Studio
# export STUDIO_JDK=$(/usr/libexec/java_home -v 1.7 | sed -e "s/\/Contents\/Home//")


#
# Postgress.app - http://postgresapp.com/
#

export PATH=/Applications/Postgres.app/Contents/Versions/latest/bin:$PATH


##
## EOF
##
